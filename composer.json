{
  "name": "cwbit/cakephp-emailqueue",
  "description": "A configurable, queue-based plugin for queuing and sending Emails in CakePHP 3",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Brescia"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": "~3.0",
    "cakephp/plugin-installer": "*",
    "cakephp/migrations": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "EmailQueue\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EmailQueue\\Test\\": "tests"
    }
  }
}
